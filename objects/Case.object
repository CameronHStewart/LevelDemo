    <validationRules>
        <fullName>Duplicate_Case</fullName>
        <active>true</active>
        <description>User must enter the parent case if the status is flagged as a duplicate.</description>
        <errorConditionFormula>ISPICKVAL( Closed_reason__c , &quot;Duplicate&quot;)&amp;&amp;
ISBLANK( ParentId)</errorConditionFormula>
        <errorDisplayField>ParentId</errorDisplayField>
        <errorMessage>Please enter the associated parent case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_fields_before_closing_the_case</fullName>
        <active>true</active>
        <description>Making closed reason, Category and Subcategory Mandatory on closing the case.</description>
        <errorConditionFormula>AND( 
TEXT( Status ) = &quot;Closed&quot;, 
OR(
ISBLANK(TEXT(Closed_reason__c)),
ISBLANK(TEXT(Category__c)),
ISBLANK(TEXT(Subcategory__c))
)
)</errorConditionFormula>
        <errorMessage>Please specify Closed Reason, Category and Subcategory when closing the case.</errorMessage>
    </validationRules>
